@article{caldwell2019,
  title = {Bicycle Commuting in an Automobile-Dominated City: How Individuals Become and Remain Bike Commuters in {{Charlotte}}, {{North Carolina}}},
  shorttitle = {Bicycle Commuting in an Automobile-Dominated City},
  author = {Caldwell, Kevin B. and Boyer, Robert H. W.},
