@article{chum2015,
  title = {The Impact of Urban Land Uses on Sleep Duration and Sleep Problems},
  author = {Chum, Antony and O'Campo, Patricia and Matheson, Flora},
  date = {2015},
  journaltitle = {The Canadian Geographer / Le G\'eographe canadien},
  volume = {59},
